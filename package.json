{
  "name": "fast-food-zero-ui",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "dev": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web --output-dir dist",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "smoke:mvp": "jest --testPathPattern=smoke-production-mvp --verbose",
    "smoke:staging": "npx ts-node scripts/smoke-staging.ts",
    "auth:sanity": "npx ts-node scripts/auth-sanity.ts",
    "auth:sanity:require401": "npx ts-node scripts/auth-sanity-require401.ts",
    "auth:sanity:require200": "npx ts-node scripts/auth-sanity-require200.ts",
    "flags:proof": "npx ts-node scripts/runtime-flag-proof.ts",
    "readonly:proof": "npx ts-node scripts/readonly-proof.ts",
    "db:migrate": "npx ts-node db/migrate.ts",
    "db:migrate:staging": "npx ts-node db/migrate.ts",
    "db:verify:staging": "npx ts-node scripts/db-verify-staging.ts",
    "metrics:prune": "npx ts-node scripts/metrics-prune.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "eas:build:dev": "eas build --profile development --platform ios",
    "eas:build:preview": "eas build --profile preview --platform ios",
    "eas:build:prod": "eas build --profile production --platform ios",
    "eas:submit:prod": "eas submit --platform ios --latest",
    "release:testflight": "eas build --profile production --platform ios --auto-submit"
  },
  "dependencies": {
    "expo": "~52.0.30",
    "expo-router": "~4.0.17",
    "expo-av": "~14.0.7",
    "expo-speech": "~12.0.2",
    "expo-camera": "~16.0.4",
    "expo-media-library": "~16.0.4",
    "expo-file-system": "~18.0.4",
    "expo-linear-gradient": "~14.0.1",
    "expo-haptics": "~13.0.1",
    "expo-status-bar": "~2.0.0",
    "expo-font": "~13.0.1",
    "expo-splash-screen": "~0.29.13",
    "expo-image-picker": "~15.0.7",
    "expo-notifications": "~0.29.9",
    "expo-device": "~6.0.2",
    "expo-location": "~17.0.1",
    "expo-contacts": "~13.0.5",
    "expo-calendar": "~13.0.5",
    "expo-sharing": "~12.0.1",
    "expo-clipboard": "~6.0.3",
    "expo-local-authentication": "~14.0.1",
    "expo-secure-store": "~13.0.2",
    "expo-background-fetch": "~12.0.1",
    "expo-task-manager": "~11.8.2",
    "expo-build-properties": "~0.12.5",
    "react": "18.3.1",
    "react-native": "0.76.3",
    "react-native-web": "~0.19.12",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-markdown-display": "^7.0.2",
    "@expo/vector-icons": "^14.0.4",
    "react-native-super-grid": "^6.0.1",
    "react-native-animatable": "^1.4.0",
    "lucide-react-native": "^0.454.0",
    "@expo-google-fonts/inter": "^0.2.3"
  },
  "optionalDependencies": {
    "pg": "^8.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "@types/react-native": "~0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.0",
    "@testing-library/react-native": "^12.4.0",
    "@testing-library/jest-native": "^5.4.3",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3",
    "babel-plugin-module-resolver": "^5.0.0"
  },
  "private": true
}