{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "npm run lint && expo export -p web",
  "outputDirectory": "dist",
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/((?!_expo/|assets/|favicon\\.ico|robots\\.txt|sitemap\\.xml).*)",
      "destination": "/"
    }
  ]
}
