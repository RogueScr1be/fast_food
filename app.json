{
  "expo": {
    "name": "Fast Food",
    "slug": "fast-food",
    "version": "1.0.0",
    "description": "Fast Food helps households make one executable dinner decision with minimal friction.",
    "privacyPolicyUrl": "https://fast-food.vercel.app/privacy",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7FCFE"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fastfood.zeroui",
      "requireFullScreen": true,
      "privacyManifests": {
        "NSPrivacyTracking": false,
        "NSPrivacyTrackingDomains": [],
        "NSPrivacyCollectedDataTypes": [],
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          }
        ]
      },
      "infoPlist": {
        "UIRequiresFullScreen": true,
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["background-fetch", "remote-notification"],
        "NSCameraUsageDescription": "This app uses the camera to scan grocery receipts for meal plan optimization.",
        "NSMicrophoneUsageDescription": "This app uses the microphone for voice commands to create meal plans.",
        "NSLocationWhenInUseUsageDescription": "This app uses location to find nearby grocery stores and restaurants."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F7FCFE"
      },
      "package": "com.fastfood",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-camera",
      "expo-location",
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar to add meal plan events."
        }
      ],
      "expo-notifications"
    ],
    "scheme": "fast-food",
    "platforms": ["ios", "android", "web"],
    "extra": {
      "eas": {
        "projectId": "26890a3f-bd10-4761-97fa-d545d6426817"
      }
    },
    "owner": "roguescr1be"
  }
}
